{
  "name": "proposalsapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": ["apps/web", "libs/ts/*", "libs/ts/emails/.react-email"],
  "scripts": {
    "build-db": "yarn workspace @proposalsapp/db db:gen:web",
    "build-emails": "yarn workspace @proposalsapp/emails build",
    "build-web": "yarn build-db && yarn workspace @proposalsapp/web build",
    "start-web": "yarn workspace @proposalsapp/web start"
  },
  "resolutions": {
    "open-graph-scraper/undici": "6.6.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.2",
    "constructs": "10.3.0",
    "dotenv": "16.4.5",
    "prettier": "^3.2.5",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": "20"
  }
}
