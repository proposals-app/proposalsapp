{
  "name": "proposalsapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/web",
    "apps/astro",
    "apps/arbitrum",
    "apps/ai_playground",
    "apps/emails/*",
    "apps/push/*",
    "libs/ts/*",
    "libs/ts/emails/.react-email"
  ],
  "scripts": {
    "build-db": "yarn workspace @proposalsapp/db db:gen:ts",
    "build-email-bulletin": "yarn build-db && yarn workspace @proposalsapp/email-bulletin build",
    "build-email-quorum": "yarn build-db && yarn workspace @proposalsapp/email-quorum build",
    "build-email-timeend": "yarn build-db && yarn workspace @proposalsapp/email-timeend build",
    "build-push-quorum": "yarn build-db && yarn workspace @proposalsapp/push-quorum build",
    "build-push-timeend": "yarn build-db && yarn workspace @proposalsapp/push-timeend build",
    "build-emails": "yarn workspace @proposalsapp/emails build",
    "build-web": "yarn build-db && yarn workspace @proposalsapp/web build",
    "build-arbitrum": "yarn build-db && yarn workspace @proposalsapp/arbitrum build",
    "build-astro": "yarn build-db && yarn workspace @proposalsapp/astro build",
    "start-email-bulletin": "yarn workspace @proposalsapp/email-bulletin start",
    "start-email-quorum": "yarn workspace @proposalsapp/email-quorum start",
    "start-email-timeend": "yarn workspace @proposalsapp/email-timeend start",
    "start-push-quorum": "yarn workspace @proposalsapp/push-quorum start",
    "start-push-timeend": "yarn workspace @proposalsapp/push-timeend start",
    "start-web": "yarn workspace @proposalsapp/web start",
    "start-arbitrum": "yarn workspace @proposalsapp/arbitrum start",
    "start-astro": "yarn workspace @proposalsapp/astro start",
    "update-deps": "./update-deps.sh"
  },
  "resolutions": {
    "undici": "6.6.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@tsconfig/recommended": "^1.0.7",
    "constructs": "10.3.0",
    "dotenv": "16.4.5",
    "prettier": "^3.3.2",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": "20"
  }
}
