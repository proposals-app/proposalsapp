  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "NEXT_OTEL_VERBOSE=1 next dev --turbopack",
    "scan": "next dev & npx react-scan@latest localhost:3000",
    "format": "prettier --check --config ../../prettier.config.mjs --ignore-path ../../.gitignore .",
    "format:fix": "prettier --write --config ../../prettier.config.mjs --ignore-path ../../.gitignore .",
    "lint": "eslint . --config ../../eslint.config.mjs",
    "lint:fix": "eslint . --config ../../eslint.config.mjs --fix",
    "start": "next start",
    "vapid": "web-push generate-vapid-keys",
    "synpress-setup": "npx synpress ./e2e/wallet-setup",
    "test-ui": "playwright test --ui",
    "anvil": "anvil --chain-id 42161 --fork-url https://arbitrum.drpc.org --block-time 1 --fork-block-number 330000000",
    "e2e:start-storybook": "storybook dev -p 6006 --no-open",
    "e2e:start-anvil": "anvil --chain-id 42161 --fork-url https://arbitrum.drpc.org --block-time 1 --fork-block-number 330000000",
    "e2e:services": "concurrently \"pnpm e2e:start-storybook\" \"pnpm e2e:start-anvil\" --names \"storybook,anvil\" --prefix-colors \"blue,green\"",
    "e2e:services-offchain": "pnpm e2e:start-storybook",
    "e2e:test-basic": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Basic\" --reporter=line",
    "e2e:test-single-choice": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Single-Choice\" --reporter=line",
    "e2e:test-approval": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Approval\" --reporter=line",
    "e2e:test-quadratic": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Quadratic\" --reporter=line",
    "e2e:test-ranked-choice": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Ranked-Choice\" --reporter=line",
    "e2e:test-weighted": "npx playwright test e2e/vote-offchain.spec.ts --grep \"Weighted\" --reporter=line",
    "e2e:test-arbitrum-core": "npx playwright test e2e/vote-onchain.spec.ts --grep \"Arbitrum Core\" --reporter=line",
    "e2e:test-arbitrum-treasury": "npx playwright test e2e/vote-onchain.spec.ts --grep \"Arbitrum Treasury\" --reporter=line",
    "e2e:offchain": "start-server-and-test e2e:start-storybook http://localhost:6006 \"pnpm synpress-setup && npx playwright test e2e/vote-offchain.spec.ts\"",
    "e2e:onchain": "pnpm e2e:start-anvil & pnpm e2e:start-storybook & start-server-and-test storybook http://localhost:6006 \"pnpm synpress-setup && npx playwright test e2e/vote-onchain.spec.ts\"",
    "e2e:offchain-ui": "start-server-and-test e2e:start-storybook http://localhost:6006 \"pnpm synpress-setup && npx playwright test e2e/vote-offchain.spec.ts --ui\"",
    "e2e:onchain-ui": "pnpm e2e:start-anvil & pnpm e2e:start-storybook & start-server-and-test storybook http://localhost:6006 \"pnpm synpress-setup && npx playwright test e2e/vote-onchain.spec.ts --ui\"",
    "e2e:ui": "start-server-and-test e2e:services-offchain http://localhost:6006 \"pnpm synpress-setup && pnpm test-ui\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --webpack-stats-json",
    "chromatic": "chromatic --config-file chromatic.config.json",
    "chromatic:ci": "chromatic --config-file chromatic.config.json --ci",
    "chromatic:only-changed": "chromatic --config-file chromatic.config.json --only-changed",
    "visual-test": "pnpm build-storybook && pnpm chromatic"
  }
